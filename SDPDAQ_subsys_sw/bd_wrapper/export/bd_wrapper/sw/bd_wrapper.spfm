<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="bd_wrapper"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
bd_wrapper
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="bd_wrapper">
        <sdx:configuration sdx:name="bd_wrapper"
                           sdx:displayName="bd_wrapper"
                           sdx:defaultProcessorGroup="standalone_ps7_cortexa9_0"
                           sdx:runtimes="cpp">
            <sdx:description>bd_wrapper</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="bd_wrapper/boot/bd_wrapper.bif"
                            sdx:readme="bd_wrapper/boot/generic.readme"
                            sdx:qemuArguments="bd_wrapper/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="freertos10_xilinx_ps7_cortexa9_0"
                            sdx:bif="bd_wrapper/boot/bd_wrapper.bif"
                            sdx:readme="bd_wrapper/boot/generic.readme"
                            sdx:qemuArguments="bd_wrapper/freertos10_xilinx_ps7_cortexa9_0/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_ps7_cortexa9_0"
                            sdx:bif="bd_wrapper/boot/bd_wrapper.bif"
                            sdx:readme="bd_wrapper/boot/generic.readme"
                            sdx:qemuArguments="bd_wrapper/standalone_ps7_cortexa9_0/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="freertos10_xilinx_ps7_cortexa9_0"
                                sdx:displayName="freertos10_xilinx_ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="freertos10_xilinx"
                        sdx:displayName="freertos10_xilinx"
                        sdx:bspLibraryPaths="bd_wrapper/freertos10_xilinx_ps7_cortexa9_0/bsplib/lib"
                        sdx:bspIncludePaths="bd_wrapper/freertos10_xilinx_ps7_cortexa9_0/bspinclude/include"
                        sdx:bspConfig="bd_wrapper/freertos10_xilinx_ps7_cortexa9_0/system.mss"
                />
            </sdx:processorGroup>
            <sdx:processorGroup sdx:name="standalone_ps7_cortexa9_0"
                                sdx:displayName="standalone_ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="bd_wrapper/standalone_ps7_cortexa9_0/bsplib/lib"
                        sdx:bspIncludePaths="bd_wrapper/standalone_ps7_cortexa9_0/bspinclude/include"
                        sdx:bspConfig="bd_wrapper/standalone_ps7_cortexa9_0/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
